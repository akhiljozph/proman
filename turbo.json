{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                "build/**"
            ],
            "env": [
                "NODE_ENV"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "start": {
            "dependsOn": [
                "build"
            ],
            "cache": false,
            "persistent": true
        },
        "generate": {
            "cache": false,
            "outputs": [
                "src/generated/**"
            ]
        },
        "migrate": {
            "cache": false,
            "dependsOn": [
                "generate"
            ]
        },
        "studio": {
            "cache": false,
            "persistent": true
        },
        "clean": {
            "cache": false
        }
    }
}